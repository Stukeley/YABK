func void Equip_TEST_ITHE()
{
	self.attribute[ATR_HITPOINTS_MAX] = self.attribute[ATR_HITPOINTS_MAX] + 5000;
	self.attribute[ATR_HITPOINTS] = self.attribute[ATR_HITPOINTS] + 5000;
};

func void UnEquip_TEST_ITHE()
{
	self.attribute[ATR_HITPOINTS_MAX] = self.attribute[ATR_HITPOINTS_MAX] - 5000;
	if(self.attribute[ATR_HITPOINTS] > (5000 + 1))
	{
		self.attribute[ATR_HITPOINTS] = self.attribute[ATR_HITPOINTS] - 5000;
	}
	else
	{
		self.attribute[ATR_HITPOINTS] = 2;
	};
};